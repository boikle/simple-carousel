{  
	"extends": "eslint:recommended",
	"parserOptions": {
	    "ecmaVersion": 6,
        "sourceType": "module"
    },
  	"env": {
        "browser": true,
		"es6": true,
		"node": true,
		"amd": true
    },
  	"rules": {
		"no-redeclare": 1,
		"no-unused-vars": 1,
	    "comma-dangle": 0,
	    "no-console": 1,
	    "no-extra-parens": 0,
	    "accessor-pairs": 0,
	    "block-scoped-var": 1,
	    "complexity": [1, 10],
	    "consistent-return": 0,
	    "curly": 1,
	    "default-case": 1,
	    "dot-notation": 0,
	    "dot-location": 0,
	    "eqeqeq": 1,
	    "guard-for-in": 1,
	    "no-alert": 0,
	    "no-caller": 0,
	    "no-div-regex": 0,
	    "no-else-return": 0,
	    "no-eq-null": 0,
	    "no-eval": 1,
	    "no-extend-native": 0,
	    "no-extra-bind": 0,
	    "no-floating-decimal": 0,
	    "no-implicit-coercion": 0,
	    "no-implied-eval": 0,
	    "no-invalid-this": 0,
	    "no-iterator": 0,
	    "no-labels": 0,
	    "no-lone-blocks": 0,
	    "no-loop-func": 1,
	    "no-multi-spaces": 1,
	    "no-multi-str": 0,
	    "no-new-func": 1,
	    "no-new-wrappers": 1,
	    "no-new": 0,
	    "no-octal-escape": 1,
	    "no-param-reassign": 0,
	    "no-process-env": 0,
	    "no-proto": 0,
	    "no-return-assign": 1,
	    "no-script-url": 0,
	    "no-self-compare": 1,
	    "no-sequences": 0,
	    "no-throw-literal": 0,
	    "no-unused-expressions": 0,
	    "no-useless-call": 0,
	    "no-void": 0,
	    "no-warning-comments": 0,
	    "radix": 0,
	    "vars-on-top": 1,
	    "wrap-iife": 0,
	    "yoda": 1,
	    "strict": 1,
	    "init-declarations": 0,
	    "no-delete-var": 0,
	    "no-label-var": 0,
	    "no-shadow": 0,
	    "no-undef-init": 0,
	    "no-undefined": 0,
	    "no-use-before-define": 1,
	    "array-bracket-spacing": 0,
	    "brace-style": 1,
	    "camelcase": 0,
	    "comma-spacing": 0,
	    "comma-style": [0, "first"],
	    "computed-property-spacing": 1,
	    "consistent-this": 0,
	    "eol-last": 1,
	    "func-names": 0,
	    "func-style": 0,
	    "id-length": 0,
	    "indent": [1, "tab"],
	    "key-spacing": [1, {"afterColon": true}],
	    "lines-around-comment": 0,
	    "linebreak-style": 0,
	    "max-nested-callbacks": 0,
	    "new-cap": 1,
	    "new-parens": 0,
	    "no-array-constructor": 0,
	    "no-continue": 0,
	    "no-inline-comments": 0,
	    "no-lonely-if": 0,
	    "no-multiple-empty-lines": 1,
	    "no-nested-ternary": 1,
	    "no-new-object": 0,
	    "no-ternary": 0,
	    "no-trailing-spaces": 1,
	    "no-underscore-dangle": 0,
	    "no-unneeded-ternary": 0,
	    "object-curly-spacing": 1,
	    "one-var": 0,
	    "operator-assignment": 1,
	    "operator-linebreak": 0,
	    "padded-blocks": 0,
	    "quote-props": 0,
	    "quotes": 0,
	    "id-match": 0,
	    "semi-spacing": 1,
	    "semi": 1,
	    "sort-vars": 0,
		"keyword-spacing": [1, { "before": true, "after": true }],
	    "space-before-blocks": 1,
	    "space-before-function-paren": [0, "always"], 
	    "space-in-parens": [1,"never",{}],
	    "space-infix-ops": 1,
	    "space-unary-ops": 0,
	    "spaced-comment": [1, "always"],
	    "wrap-regex": 1,
	    "arrow-parens": 0,
	    "arrow-spacing": 0,
	    "constructor-super": 0,
	    "generator-star-spacing": 0,
	    "no-class-assign": 0,
	    "no-const-assign": 0,
	    "no-this-before-super": 0,
	    "no-var": 0,
	    "object-shorthand": 0,
	    "prefer-const": 0,
	    "prefer-spread": 0,
	    "prefer-reflect": 0,
	    "max-depth": [1, { "max": 4 }],
	    "max-len": 1,
	    "max-params": 0,
	    "max-statements": 0,
	    "no-bitwise": 0,
	    "no-plusplus": 1
  }
}
